            "    ]",
            "}"
        ],
        "tag/entity_type": [
            "{",
            "    \"values\": [",
            "        %cursor%",
            "    ]",
            "}"
        ],
        "tag/fluid": [
            "{",
            "    \"values\": [",
            "        %cursor%",
            "    ]",
            "}"
        ],
        "tag/function": [
            "{",
            "    \"values\": [",
